{
    "version": 3,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/app.js"
      },
      {
        "src": "/(.*)", 
        "dest": "/index.html"
      }
    ]
  }
  